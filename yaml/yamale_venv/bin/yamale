#!/home/tom/src/python/yaml/yamale_venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'yamale==3.0.1','console_scripts','yamale'
__requires__ = 'yamale==3.0.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('yamale==3.0.1', 'console_scripts', 'yamale')()
    )
